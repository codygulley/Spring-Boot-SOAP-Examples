            <xs:sequence>
                <xs:element name="randomNumber" type="xs:integer"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="randomStringRequest">
        <xs:complexType>
            <xs:sequence>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="randomStringResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="randomString" type="xs:string"/>
